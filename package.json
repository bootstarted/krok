    "eslint-plugin-filenames": "^0.2.0",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-lodash-fp": "^1.2.0",
    "eslint-plugin-react": "^5.1.1",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
